CHECKSETS qt5,c++,foss

#KDAB-specific checks
EXTRA kdabcopyright-reuse,kdabcontactus,fosslicense-reuse

#additional checks
#EXTRA defines,null

#exclude checks now being done by clazy or clang-tools
EXCLUDE includes,strings,explicit,normalize,passbyvalue,operators,nullstrcompare,nullstrassign,doublequote_chars,qobject,sigsandslots,staticobjects,inline,postfixop
#exclude more checks
EXCLUDE style

#skip the combined header
SKIP /src/KDReports/KDReports.h

#skip other cmake
SKIP Doxyfile.cmake
#skip the borrowed code in the cmake subdir
SKIP /cmake/ECM/|/cmake/KDAB/
